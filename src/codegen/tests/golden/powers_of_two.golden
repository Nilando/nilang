fn raise(n, x) {
  if x == 0 {
    return 1;
  }

  x = x - 1;

  while x > 0 {
    n = n * n;

    x = x - 1;
  }

  return n;
}

p = 0;
while p < 12 {
  print(raise(2, p));
  p = p + 1;
}

