truthy_vals = [
  0,
  1,
  1.234,
  [],
  {},
  "",
  "false",
  fn(a,b,c) {},
  true,
  #sym,
  #null,
  #false,
];

falsy_vals = [
  false,
  null,
];

poor_mans_assert = true;

i = 0;
while i < len(truthy_vals) {
  v = truthy_vals[i];
